{
  "name": "rag-chatbot-2",
  "version": "1.0.0",
  "description": "Production-ready RAG chatbot with Fastify and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest tests/unit",
    "test:watch": "jest tests/unit --watch",
    "test:coverage": "jest tests/unit --coverage",
    "test:notion": "tsx scripts/test-notion-integration.ts",
    "test:openai": "tsx scripts/test-openai-integration.ts",
    "test:embedding": "tsx scripts/test-embedding-integration.ts",
    "test:pinecone": "tsx scripts/test-pinecone-integration.ts",
    "test:full-pipeline": "tsx scripts/test-full-pipeline.ts",
    "test:rag-pipeline": "tsx scripts/test-rag-pipeline.ts",
    "test:streaming-api": "tsx scripts/test-streaming-api.ts",
    "debug:vector-search": "tsx scripts/debug-vector-search.ts"
  },
  "keywords": [
    "rag",
    "chatbot",
    "fastify",
    "typescript",
    "openai",
    "pinecone"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.4",
    "@notionhq/client": "^4.0.1",
    "@pinecone-database/pinecone": "^6.1.1",
    "dotenv": "^17.2.0",
    "fastify": "^4.28.1",
    "graphql-request": "^7.1.2",
    "markdown-it": "^14.1.0",
    "multer": "^2.0.1",
    "node-html-parser": "^7.0.1",
    "notion-to-md": "^3.1.9",
    "openai": "^5.12.0",
    "pino": "^9.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/markdown-it": "^14.1.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "axios": "^1.11.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
